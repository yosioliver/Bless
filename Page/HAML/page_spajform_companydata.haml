                    %input#RadioButtonPolicyHolderHubunganKaryawan.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderTertanggungRelationship", :value => "100juta" }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderHubunganKaryawan" } Perusahaan / Karyawan
                    %div.Separator
                    %input#RadioButtonPolicyHolderHubunganLainnya.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderTertanggungRelationship", :value => "100juta1miliar"  }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderHubunganLainnya" } Lainnya
                    %input#TextPolicyHolderCIN.Medium.Single.PositionerRight{ :type => "text" }
                %br
                %label.Medium.Single.PositionerLeft{ :for => "TextPolicyHolderHubunganDenganCalonTertanggung" } Tujuan Pembelian Asuransi
                %div.RadioButton.PositionerLeft
                    %input#RadioButtonPolicyHolderTujuanTabungan.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderTujuanPembelianAsuransi", :value => "100juta" }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderTujuanTabungan" } Tabungan
