                    %input#RadioButtonPolicyHolderLainnya.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderCompanyType", :value => "yes" }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderCompanyTypeOther" } Lainnya
                    %input#RadioButtonPolicyHolderCompanyTypeOther.Medium.Single.PositionerRight{ :type => "text" }
                %br
                %div.ContainerFit
                    %label.Medium.Single.Item{ :for => "TextPolicyHolderCompanyNoAnggaranDasar" } Nomor Anggaran Dasar/Akta Pendirian
                    %input#TextPolicyHolderCompanyNoAnggaranDasar.Full.Single.Item{ :type => "text" }
                
                %label.Medium.Single.PositionerLeft{ :for => "DatePolicyHolderCompanyNoAnggaranDasar" } Tanggal Berlaku
                %input#DatePolicyHolderCompanyNoAnggaranDasar.Medium.Single.PositionerLeft{ :type => "text" }
                %br
                %div.ContainerFit
                    %label.Medium.Single.Item{ :for => "TextPolicyHolderCompanyNoSIUP" } Nomor Surat Ijin Usaha
                    %input#TextPolicyHolderCompanyNoSIUP.Full.Single.Item{ :type => "text" }
                %label.Medium.Single.PositionerLeft{ :for => "TextPolicyHolderTanggalBerlaku" } Tanggal Berlaku
                %input#TextPolicyHolderTanggalBerlaku.Medium.Single.PositionerLeft{ :type => "text" }
                %br
                %div.ContainerFit
                    %label.Medium.Single.Item{ :for => "TextPolicyHolderCompanyNoTDP" } Nomor TDP
                    %input#TextPolicyHolderTanggalBerlaku.Full.Single.Item{ :type => "text" }
                %label.Medium.Single.PositionerLeft{ :for => "DatePolicyHolderCompanyTDPDateOfExpired" } Tanggal Berlaku
                %input#DatePolicyHolderCompanyTDPDateOfExpired.Medium.Single.PositionerLeft{ :type => "text" }
