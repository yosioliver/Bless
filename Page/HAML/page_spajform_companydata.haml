                %br
                
                %div.ContainerFit
                    %label.Medium.Single.Item{ :for => "TextPolicyHolderCompanyAddress" } Nama lengkap (sesuai dengan identitas)
                    %input#TextPolicyHolderCompanyAddress.Full.Single.Item{ :type => "text" }
                %div.ContainerFit
                    %label.Medium.Single.Item{ :for => "TextPolicyHolderCompanyAddress" } Jabatan / Pangkat / Golongan
                    %input#TextPolicyHolderCompanyAddress.Full.Single.Item{ :type => "text" }
                    
                %label.Medium.Single.PositionerLeft{ :for =>"TextPolicyHolderCIN" } Nomor Identitas Diri
                %input#TextPolicyHolderCIN.Medium.Single.PositionerLeft{ :type => "text" }
                
                %input#TextPolicyHolderCIN.Medium.Single.PositionerRight{ :type => "text" }
                %label.Medium.Single.PositionerRight{ :for =>"TextPolicyHolderCIN" } Masa Berlaku
                %br
                
