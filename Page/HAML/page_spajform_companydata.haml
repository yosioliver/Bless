                    %input#RadioButtonPolicyHolderTujuanProteksi.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderTujuanPembelianAsuransi", :value => "100juta1miliar"  }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderTujuanProteksi" } Proteksi
