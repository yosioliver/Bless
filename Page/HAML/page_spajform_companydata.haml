                    %input#RadioButtonPolicyHolderTujuanInvestasi.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderTujuanPembelianAsuransi", :value => "100juta1miliar"  }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderTujuanInvestasi" } Investasi
                    %div.Separator
                    %input#RadioButtonPolicyHolderTujuanPendidikan.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderTujuanPembelianAsuransi", :value => "100juta1miliar"  }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderTujuanPendidikan" } Pendidikan
                    %div.Separator
                    %input#RadioButtonPolicyHolderTujuanLainnya.Medium.Single{ :type => "radio", :name => "RadioButtonPolicyHolderTujuanPembelianAsuransi", :value => "100juta1miliar"  }
                    %label.RadioButton{ :for => "RadioButtonPolicyHolderTujuanLainnya" } Lainnya
                    %input#TextPolicyHolderCIN.Medium.Single.PositionerRight{ :type => "text" }
                    